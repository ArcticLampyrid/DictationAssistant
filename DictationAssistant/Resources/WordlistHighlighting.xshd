<?xml version="1.0" encoding="utf-8"?>
<SyntaxDefinition name="WordlistHighlighting" xmlns="http://icsharpcode.net/sharpdevelop/syntaxdefinition/2008">
  <Color name="Comment" foreground="Green" />
  <Color name="Heading" foreground="White" background="Black" />
  <RuleSet>
    <Rule color="Heading">
      ^[#].*$
    </Rule>
    <Span color="Comment" begin="//" />
  </RuleSet>
</SyntaxDefinition>